{
  "name": "react-dom-navigator",
  "version": "1.0.0",
  "description": "A React Typescript module that provides a dom tree nodes visualization",
  "keywords": [
    "react-bootstrap",
    "bootstrap",
    "widget",
    "dom-tree",
    "react-dom",
    "typescript"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "npx webpack serve",
    "build": "tsc && copyfiles -u 1 ./src/EmbedDomWidget/*.css ./dist/src",
    "build:embeded": "npx webpack --config webpack.config.js"
  },
  "author": "dsnihurskyi",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dsnihurskyi/react-dom-navigator"
  },
  "dependencies": {
    "bootstrap": "^5.3.0"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.2.6",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.8.1",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.87.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
