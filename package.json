{
  "name": "react-dom-navigator",
  "version": "1.0.0",
  "description": "A React Typescript module that provides a dom tree nodes visualization",
  "keywords": [
    "react-bootstrap",
    "bootstrap",
    "widget",
    "dom-tree",
    "react-dom",
    "typescript"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "npx webpack serve",
    "build": "tsc && copyfiles -u 1 ./src/EmbedDomWidget/*.css ./dist/src",
    "build:embeded": "webpack-cli --mode production",
    "lint": "npx eslint src/** embedable.tsx --ext .ts,.tsx"
  },
  "author": "dsnihurskyi",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dsnihurskyi/react-dom-navigator"
  },
  "dependencies": {
    "bootstrap": "^5.3.0"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.2.6",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "copy-webpack-plugin": "^11.0.0",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.8.1",
    "eslint": "^8.42.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.3",
    "typescript": "^5.0.4",
    "webpack": "^5.87.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
